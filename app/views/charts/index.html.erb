
<h2>History</h2>

<script type="text/javascript" charset="utf-8">
  $(function() {
    new Highcharts.Chart({
      chart: {
        renderTo: "elChart"
      },
      title: {
        text: "The Title"
      },
      xAxis: {
        type: "datetime",
        title: {
          text: "x-axis title"
        }
      },
      yAxis: {
        title: {
          text: "yAxis title"
        }
      },
      series: [{
        pointInterval: <%= 1.day * 1000 %>,
        pointStart: <%= 3.weeks.ago.to_i * 1000 %>,
        data: [1, 3, 8, 10]
      }]
    });
  });
</script>

<div id="elChart" style="height: 500px;"></div>
