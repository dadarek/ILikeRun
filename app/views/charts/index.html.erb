
<h2>Last 2 Weeks</h2>

<script type="text/javascript" charset="utf-8">
  $(function() {
    new Highcharts.Chart({
      chart: {
        renderTo: "elChart",
        marginLeft: 50,
        marginRight: 50
      },
      title: {
        text: null
      },
      xAxis: {
        type: "datetime",
        tickInterval: 24 * 3600 * 1000
      },
      yAxis: {
        title: {
          text: "Minutes Ran"
        }
      },
      series: [{
        pointInterval: 24 * 3600 * 1000,
        pointStart: Date.UTC(2012, 3, 30),
        data: [1, 3, 8, 10, 6],
        name: null
      }],
      tooltip: {
				formatter: function() {
					var point = this.points[0];
					return '<b>'+ Highcharts.dateFormat('%A, %B %e, %Y', this.x) +'</b><br/>'+
						point.y +' minutes';
				},
				shared: true
			},
      legend: { enabled: false }
    });
  });
</script>

<div id="elChart" style="height: 500px;"></div>
